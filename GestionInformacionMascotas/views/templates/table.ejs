<table class="table table-striped ">
    <thead class="table-dark">
        <tr>
          <th scope="col">Nombre</th>
          <th scope="col">Dueño</th>
          <th scope="col">Especie</th>
          <th scope="col">Genero</th>
          <th scope="col">Acciones</th>
        </tr>
      </thead>
    <tbody id="tbody">
      <% animals.map(value => { %>
        <tr>
            <td><%= value.mascota %></td>
            <td><%= value.nombredueño %>  <%= value.apellidosdueño %></td>
            <td><%= value.especie %></td>
            <td><%= value.genero %></td>
            <td> 
              <button type="button" class="btn btn-primary" data-bs-toggle="modal" onclick="descripcion('<%= value.mascota%>','<%= value.especie%>','<%= value.genero%>','<%= value.enfermedad%>','<%= value.nombredueño%>','<%= value.apellidosdueño%>','<%= value.cedula%>','<%= value.direccion%>','<%= value.email%>','<%= value.telefono%>')"data-bs-target="#exampleModal">
                D
              </button>
              <button type="submit" class="btn btn-danger" onclick="eliminar('<%= value.mascota %>','<%= value.cedula %>')" >E</button>
              <button type="submit" class="btn btn-warning" onclick="actualizar('<%= value.mascota %>','<%= value.cedula %>')" >A</button>
            </td>
        </tr>
    <%})%>
    </tbody>
  </table>